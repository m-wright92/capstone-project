<div id="user_list">
  <% if @users %>
    <% @users.each do |user| %>
      <% if user != current_user %>
        <div class="cards">
          <%= link_to user.full_name, user_path(user), :class => "card" %>
        </div>
      <% end %>
    <% end %>
  <% else %>
    <p>No users found</p>
  <% end %>
</div>
